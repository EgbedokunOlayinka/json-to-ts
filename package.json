{
  "name": "json-to-ts-cli",
  "version": "1.0.0",
  "description": "a package that converts json data to typescript interfaces",
  "main": "build/src/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "start:prod": "npm run build && node build/index.js",
    "start": "ts-node --esm ./src/index.ts"
  },
  "bin": {
    "jtsc": "./build/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EgbedokunOlayinka/json-to-ts-cli.git"
  },
  "keywords": [
    "json",
    "ts",
    "json-to-ts"
  ],
  "author": "Egbedokun Olayinka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EgbedokunOlayinka/json-to-ts/issues"
  },
  "homepage": "https://github.com/EgbedokunOlayinka/json-to-ts#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "inquirer": "^9.2.7",
    "json-to-ts": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
